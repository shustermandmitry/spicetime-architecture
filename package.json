    "@stoplight/spectral-cli": "^6.11.0",
    "c4builder": "^0.2.16",
    "gatsby": "^5.12.0",
    "gatsby-plugin-mdx": "^5.12.0",
    "gatsby-remark-mermaid": "^4.0.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-theme-documentation": "^0.0.27",
    "graphql": "^16.8.0",
    "graphql-docs": "^0.2.0",
    "markdownlint-cli": "^0.35.0",
    "markdown-link-check": "^3.11.2",
    "mermaid": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "openapi-types": "^12.1.3",
    "redoc-cli": "^0.13.21",
    "remark-cli": "^11.0.0",
    "remark-lint": "^9.1.2",
    "remark-preset-lint-recommended": "^6.1.3",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typedoc-plugin-mermaid": "^1.10.0"
  },
  "packageManager": "pnpm@10.0.0-beta.1+sha512.629de0531b9ae9a3f8e372d014ef8f5a57906d9a48095ced54bbfbd246b4136381478032c8d13819fd1eedde8330517a799ea6756eedd9a136e36524fa3083cf"
