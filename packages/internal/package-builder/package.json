{
  "name": "@internal/package-builder",
  "version": "0.1.0",
  "description": "A TypeScript-powered package for building and managing reusable templates.",
  "main": "src/cli.ts",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm src/cli.ts",
    "build": "tsup src/cli.ts --format esm,cjs --out-dir dist",
    "test": "vitest",
    "docs": "typedoc"
  },
  "dependencies": {
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "ts-node": "^10.0.0",
    "tsup": "^7.0.0",
    "typedoc": "^0.25.0",
    "vitest": "^0.34.0"
  }
}
