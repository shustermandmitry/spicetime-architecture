{
  "entryPoints": ["src"],
  "out": "docs/typedoc",
  "name": "Content Aggregator Documentation",
  "tsconfig": "tsconfig.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mermaid"],
  "categorizeByGroup": true,
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeExternals": true,
  "readme": "README.md",
  "media": "media"
}